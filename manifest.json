{
  "manifest_version": 2,

  "name": "ThingamaTAB",
  "description": "Open a tab and have fun!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "chrome_url_overrides": {
  	"newtab" : "maintab.html"

  },

    "content_security_policy":
         "script-src 'self' 'unsafe-eval' object-src 'self'"
}
